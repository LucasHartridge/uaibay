@model List<UAIBay.WebSite.ViewModel.CategoriaViewModels>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h2>Index</h2>
<table>
@foreach (var categoriaViewModel in Model)
{
    <tr>
        <td>@categoriaViewModel.IdCategoria</td>
        <td>@categoriaViewModel.Nombre</td>
    </tr>
}
</table>
