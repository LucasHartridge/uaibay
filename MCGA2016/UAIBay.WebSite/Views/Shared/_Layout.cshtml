@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>UAIBay</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/animate.css" rel="stylesheet" />
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/main.css" rel="stylesheet" />
    <link href="~/Content/prettyPhoto.css" rel="stylesheet" />
    <link href="~/Content/price-range.css" rel="stylesheet" />
    <link href="~/Content/responsive.css" rel="stylesheet" />
    <link rel="shortcut icon" href="images/ico/favicon.ico">
    <link rel="stylesheet" href="http://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.4.0/css/font-awesome.min.css">
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="~/Content/images/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="~/Content/images/ico/apple-touch-icon-114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="~/Content/images/ico/apple-touch-icon-72-precomposed.png">
    <link rel="apple-touch-icon-precomposed" href="~/Content/images/ico/apple-touch-icon-57-precomposed.png">
    <link rel="shortcut icon" type="image/x-icon" href="~/Content/images/favicon/favicon32x32.ico" />
</head>
<body>
    <header id="header">
        <!--header-->
        <div class="header_top">
            <!--header_top-->
            <div class="container">
                <div class="row">
                    <div class="col-sm-6">
                        <div class="contactinfo">
                            <ul class="nav nav-pills">
                                <li><a href="#"><i class="fa fa-phone"></i> 011 4661-6986</a></li>
                                <li><a href="#"><i class="fa fa-envelope"></i> uaibay@hotmail.com</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="social-icons pull-right">
                            <ul class="nav navbar-nav">
                                <li><a href="https://es-es.facebook.com/uai.elfuturososvos" target="_blank"><i class="fa fa-facebook"></i></a></li>
                                <li><a href="https://twitter.com/uainow?lang=es" target="_blank"><i class="fa fa-twitter"></i></a></li>
                                <li><a href="https://ar.linkedin.com/edu/universidad-abierta-interamericana-10086" target="_blank"><i class="fa fa-linkedin"></i></a></li>
                                <li><a href="https://dribbble.com/" target=" _blank"><i class="fa fa-dribbble"></i></a></li>
                                <li><a href="https://plus.google.com/about?hl=es" target="_blank"><i class="fa fa-google-plus"></i></a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div><!--/header_top-->

        <div class="header-middle">
            <!--header-middle-->
            <div class="container">
                <div class="row">
                    <div class="col-sm-4">
                        <div class="logo pull-left">
                            <a href="index.html"><img src="~/Content/images/home/Logo.Pequeño.png" alt="" /></a>
                        </div>
                        <div class="btn-group pull-right">
                            <div class="btn-group">
                                <button type="button" class="btn btn-default dropdown-toggle usa" data-toggle="dropdown">
                                    Español
                                    <span class="caret"></span>
                                </button>
                                <ul class="dropdown-menu">
                                    <li><a href="#">Ingles</a></li>
                                    <li><a href="#">Frances</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-8">
                        <div class="shop-menu pull-right">
                            <ul class="nav navbar-nav">
                                <li><a href="#"><i class="fa fa-user"></i> Cuenta</a></li>
                                @*<li><a href="#"><i class="fa fa-star"></i> Wishlist</a></li>*@
                                @*<li><a href="checkout.html"><i class="fa fa-crosshairs"></i> Checkout</a></li>*@
                                <li><a href="cart.html"><i class="fa fa-shopping-cart"></i> Carrito</a></li>
                                <li><a href="/Account/Login"><i class="fa fa-lock"></i> Login</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div><!--/header-middle-->

        <div class="header-bottom">
            <!--header-bottom-->
            <div class="container">
                <div class="row">
                    <div class="col-sm-9">
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                                <span class="sr-only">Toggle navigation</span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                        </div>
                        <div class="mainmenu pull-left">
                            <ul class="nav navbar-nav collapse navbar-collapse">
                                <li><a href="/Home/Index" class="active">Inicio</a></li>
                                <li class="dropdown">
                                    <a href="#">Tienda<i class="fa fa-angle-down"></i></a>
                                    <ul role="menu" class="sub-menu">
                                        <li><a href="checkout.html">Catalogo</a></li>
                                    </ul>
                                </li>
                                <li class="dropdown">
                                    <a href="#">Acerca de<i class="fa fa-angle-down"></i></a>
                                    <ul role="menu" class="sub-menu">
                                        <li><a href="/Home/Contacto">Contacto</a></li>
                                        <li><a href="/Home/NuestraInformacion">Nuestra informacion</a></li>
                                        <li><a href="/Home/FAQS">FAQ's</a></li>
                                    </ul>
                                </li>
                                @*<li><a href="404.html">404</a></li>*@
                                @*<li><a href="contact-us.html">Contacto</a></li>*@
                            </ul>
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <div class="search_box pull-right">
                            <input type="text" placeholder="Buscar" />
                        </div>
                    </div>
                </div>
            </div>
        </div><!--/header-bottom-->
    </header><!--/header-->
    <section>
        <div class="container">
            @RenderBody()
         </div>
</section>
    <footer id="footer">
        <!--Footer-->
        <div class="footer-widget">
            <div class="container">
                <div class="companyinfo">
                    <h2><span>uai</span>-bay</h2>
                    <p></p>
                </div>
                <div class="row">

                    <div class="col-sm-3">

                        <div class="single-widget">
                            <h2>Catalogo</h2>
                            <ul class="nav nav-pills nav-stacked">
                                <li><a href="#">Almacen</a></li>
                                <li><a href="#">Panificados</a></li>
                                <li><a href="#">Carnes</a></li>
                                <li><a href="#">Bebidas</a></li>
                                <li><a href="#">Congelados</a></li>
                                <li><a href="#">Limpieza</a></li>
                                <li><a href="#">Importados</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <div class="single-widget">
                            <h2>Acerca de</h2>
                            <ul class="nav nav-pills nav-stacked">
                                <li><a href="/Home/NuestraInformacion">Nuestra informacion</a></li>
                                <li><a href="/Home/FAQS">FAQ'S</a></li>
                                <li><a href="/Home/Contacto">Contactenos</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <div class="single-widget">
                            <h2>Nuestras novedades</h2>
                            <form action="#" class="searchform">
                                <input type="text" placeholder="Email" />
                                <button type="submit" class="btn btn-default"><i class="fa fa-arrow-circle-o-right"></i></button>
                                <p>Obtene nuestras mas recientes novedades de nuestro sitio...</p>
                            </form>
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <div class="address">
                            <img src="~/Content/images/home/map.png" alt="" />
                            <p>3550 Arias,Castelar,Buenos Aires,Argentina</p>
                        </div>
                    </div>
                    
                </div>
            </div>
        </div>

        <div class="footer-bottom">
            <div class="container">
                <div class="row">
                    <p class="pull-left">Copyright © 2016 UAIBay  Todos los derechos reservados.</p>
                    <p class="pull-right">Diseñado por los 4 Fantasticos</p>
                </div>
            </div>
        </div>

    </footer><!--/Footer-->
    @RenderSection("scripts", required: false)
    <script src="~/Content/js/jquery.js"></script>
    <script src="~/Content/js/bootstrap.min.js"></script>
    <script src="~/Content/js/jquery.scrollUp.min.js"></script>
    <script src="~/Content/js/price-range.js"></script>
    <script src="~/Content/js/jquery.prettyPhoto.js"></script>
    <script src="~/Content/js/main.js"></script>
</body>
</html>
